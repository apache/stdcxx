        "<M><a:><r><z>";"<A><p><r><.>";/
        "<M><a><i><SP>";"<J><u><n><i>";/
        "<J><u><l><i>";"<A><u><g><.>";/
        "<S><e><p><.>";"<O><k><t><.>";/
        "<N><o><v><.>";"<D><e><z><.>"
